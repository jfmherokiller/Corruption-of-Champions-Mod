                    this.combatEvent(this.anacondaBoss);
            }
            if (ID == this.CentralSquare || ID == this.WesternSquare || ID == this.EasternSquare || ID == this.SouthernSquare) {
                if (this.dayTime()) {
                    this.randomNPC(1);
                    this.ambientTravelScene(ID);
                    this.general();
                }
                else {
                    switch (this.dXY(1, 5)) {
                        case 1:
                            switch (this.LVL < 5 || this.dXY(1, 2) == 1) {
