                case 1:
                case 2:
                case 3:
                case 4:
                case 5:
                case 21:
                case 22:
                case 23:
                    _loc1_ = "night";
                    break;
                case 6:
                case 7:
                case 8:
                case 9:
                    _loc1_ = "morning";
                    break;
                case 10:
                case 11:
                case 12:
                case 13:
                case 14:
                case 15:
                    _loc1_ = "day";
                    break;
                case 16:
                case 17:
                case 18:
                case 19:
                case 20:
